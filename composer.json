			"smolowik\\Propel\\Behavior\\l10n\\tests\\" : "tests"
		}
	},
	"require" : {
		"php" : ">=5.4",
        "ext-intl" : "*",
		"propel/propel" : "~2@dev"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4",
		"propel/propel" : "dev-master@dev"
	},
	"extra" : {
		"name" : "l10n",
		"class" : "\\smolowik\\Propel\\Behavior\\l10n\\L10nBehavior"
	}
}
