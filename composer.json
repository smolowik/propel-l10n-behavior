{
	"name" : "smolowik/propel-l10n-behavior",
	"type" : "propel-behavior",
	"license" : "MIT",
	"keywords" : [
		"propel",
		"behavior",
		"l10n"
	],
	"autoload" : {
		"psr-4" : {
			"smolowik\\Propel\\Behavior\\l10n\\" : "src"
		}
